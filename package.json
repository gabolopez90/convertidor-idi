{
  "name": "Convertidor IDI",
  "author": "Gabriel Lopez",
  "description": "Guardar y leer IDI y $ de una base de datos local para operativos sin internet",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "better-sqlite3": "^7.6.2",
    "bootstrap": "^5.2.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "both": "concurrently \"npm start\" \"npm run electron-dev\"",
    "electron-pack": "npm run build --em.main=public/electron.js",
    "preelectron-pack": "npm run build",
    "postinstall": "electron-builder install-app-deps",
    "pack": "npm run build && electron-builder -w --x64 --ia32 -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "electron": "^19.1.5",
    "electron-builder": "^23.6.0",
    "foreman": "^3.0.1"
  },
  "build": {
    "appId": "com.app.prueba",
    "productName": "Calculadora IDI",
    "files": [
      "build/**",
      "node_modules/**"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis"
    }
  }
}
